#include <stdio.h>
   
int main() {
   
   int C/*coluna  a ser  considerada*/, i, j; 
char T;//S ou M, soma ou media, 
float M[12][12], soma=0; 
scanf("%d %c", &C, &T);
if(C>=0&&C<=11){
for(i=0;i<12;i++){
    for(j=0;j<12;j++){
        scanf("%f", &M[i][j]);
        if(j==C)
        soma+=M[i][j];
    }
}
if(T=='S')
printf("%.1f\n", soma);
 /*se T for S exibe somente a soma.
 caso contrario pega a soma e divive por 12 que Ã© o numero de linhas*/
else if(T=='M')
printf("%.1f\n", soma/12.0);
}
   
    return 0;
}
