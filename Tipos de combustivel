#include <stdio.h>
   
int main() {
   
    int N=0,a=0, g=0, d=0;
  
 while(N!=4){//enquanto N for diferente de 4 o programa funciona
    scanf("%d", &N);
    if(N==1)
    a++;
    else if(N==2)
    g++;
    else if(N==3)
    d++;
 }
 printf("MUITO OBRIGADO\nAlcool: %d\nGasolina: %d\nDiesel: %d\n", a,g,d);//exibir as porcentagens
    return 0;
}
